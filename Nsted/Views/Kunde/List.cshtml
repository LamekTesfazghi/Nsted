@model List<Nsted.Models.Kunde>

<link rel="stylesheet" href="~/css/KundeList.css" />

<h2>Kundeoversikt</h2>

<table class="table">
    <thead>
        <tr>
            <th>Kundenr</th>
            <th>Fornavn</th>
            <th>Etternavn</th>
            <th>Telefon</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var customer in Model)
        {
            <tr>
                <td>@customer.Id</td>
                <td>@customer.Fornavn</td>
                <td>@customer.Etternavn</td>
                <td>@customer.Telefon</td>
                <td>@customer.Email</td>
            </tr>
        }
    </tbody>
</table>
